<?xml version="1.0" encoding="utf-8" ?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <%_ sitemap.forEach(function(item){ _%>
    <url>
      <loc><%= item.loc %></loc>
      <priority><%= item.priority %></priority>
      <lastmod><%= item.lastmod %></lastmod>
    </url>
  <%_ }) _%>
</urlset>
